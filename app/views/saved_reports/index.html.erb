<p id="notice"><%= notice %></p>

<h1>Saved Reports</h1>

<table>
  <thead>
    <tr>
      <th>Email</th>
      <th>Name</th>
      <th>Casedate</th>
      <th>Linktofile</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @saved_reports.each do |saved_report| %>
      <tr>
        <td><%= saved_report.email %></td>
        <td><%= saved_report.name %></td>
        <td><%= saved_report.caseDate %></td>
        <td><%= saved_report.linkToFile %></td>
        <td><%= link_to 'Show', saved_report %></td>
        <td><%= link_to 'Edit', edit_saved_report_path(saved_report) %></td>
        <td><%= link_to 'Destroy', saved_report, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Saved Report', new_saved_report_path %>
