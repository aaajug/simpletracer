<p id="notice"><%= notice %></p>

<h1>Manager Accounts</h1>

<table>
  <thead>
    <tr>
      <th>Manager ID</th>
      <th>Added by</th>
      <th>Adden On</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @managers.each do |manager| %>
      <tr>
        <td><%= manager.managerId %></td>
        <td><%= manager.addedBy %></td>
        <td><%= manager.created_at %></td>
        <td><%= link_to 'Edit', edit_log_path(manager) %></td>
        <td><%= link_to 'Destroy', manager, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>